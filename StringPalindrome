s = list(input())
i = 0
j = len(s)-1
while(i<j):
    if(s[i] == s[-i-1]):
        i = i + 1
        j = j-1
        palindrome = "true"
    else:
        palindrome= "false"
        break
if(palindrome == "true"):
    print("String is palindrome")
else:
    print("String not Palindrome")
    
//SECOND APPROACH
s = list(input())
if(s == s[::-1]):
    print("String is palindrome")
else:
    print("String is not palindrome")
